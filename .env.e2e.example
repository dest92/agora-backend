# E2E Test Configuration
# Copy to .env.e2e and fill with real values

# Gateway URL
GATEWAY_URL=http://localhost:3000

# JWT Token (from Supabase auth - DO NOT LOG THIS)
JWT_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your-jwt-token-here

# Redis URL (supports redis:// and rediss:// for TLS)
REDIS_URL=redis://localhost:6379

# Optional: Pre-existing IDs (if not provided, will be created by bootstrap)
BOARD_ID=
WORKSPACE_ID=
SESSION_ID=

# User ID (extracted from JWT automatically)
USER_ID=
